{
  "name": "nest-study",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
    "prestart:prod": "cross-env NODE_ENV=production rimraf dist && tsc",
    "start:prod": "cross-env NODE_ENV=production node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "webpack": "webpack --config webpack.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^5.7.3",
    "@nestjs/core": "^5.7.3",
    "@nestjs/graphql": "^5.5.7",
    "@nestjs/mongoose": "^5.2.2",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.3.0",
    "apollo-server-express": "^2.14.2",
    "cache-manager": "^2.9.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.9.1",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.0",
    "csurf": "^1.9.0",
    "dotenv": "^6.2.0",
    "express-rate-limit": "^3.4.0",
    "express-session": "^1.15.6",
    "graphql": "^14.1.1",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.4",
    "joi": "^14.3.1",
    "mongoose": "^5.7.5",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "nest-log4js": "^0.0.5",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.3",
    "rxjs": "^6.4.0",
    "typeorm": "^0.2.25",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.7.3",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.9",
    "@types/joi": "^14.3.2",
    "@types/mongoose": "^5.3.21",
    "@types/node": "^11.10.5",
    "@types/supertest": "^2.0.7",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "supertest": "^3.4.2",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.13.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
